{
  "name": "build-tools",
  "main": "./lib/main",
  "version": "4.5.13",
  "description": "Build your projects in atom",
  "repository": "https://github.com/fstiewitz/build-tools-cpp",
  "license": "MIT",
  "engines": {
    "atom": ">=1.13.0-beta0 <2.0.0"
  },
  "dependencies": {
    "ps-tree": "^1.0.0",
    "atom-space-pen-views": "^2.0.0",
    "fs-plus": "^3.0.0",
    "season": "^6.0.0",
    "xregexp": "^3.0.0"
  },
  "keywords": [
    "build",
    "compile",
    "projects"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    },
    "build-tools.input": {
      "versions": {
        "1.0.0": "provideInput"
      }
    },
    "build-tools.console": {
      "versions": {
        "1.0.0": "provideConsole"
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinter"
      }
    },
    "build-tools.modifier": {
      "versions": {
        "^1.0.0": "consumeModifierModule"
      }
    },
    "build-tools.profile": {
      "versions": {
        "1.0.0": "consumeProfileModuleV1",
        "1.1.0": "consumeProfileModuleV2",
        "^1.2.0": "consumeProfileModuleV3"
      }
    },
    "build-tools.provider": {
      "versions": {
        "^1.0.0": "consumeProviderModule"
      }
    },
    "build-tools.environment": {
      "versions": {
        "^1.0.0": "consumeEnvironmentModule"
      }
    },
    "build-tools.stream-modifier": {
      "versions": {
        "^1.0.0": "consumeStreamModifier"
      }
    },
    "build-tools.output": {
      "versions": {
        "^1.0.0": "consumeOutputModule"
      }
    }
  }
}
